#include "stm32f4xx.h"
#include "stm32f4_discovery.h"

GPIO_Config(void)
{
	RCC->CR |= 0x00030000; // HSEON and HSEONRDY enable
	while(!(RCC->CR & 0x00020000)); // is HSEONRDY?
	RCC->CR |= 0x00080000; // Clock Security System enable
	RCC->PLLCFGR |= 0x00400000; // PLL HSEON enable



}

int main(void)
{
  while (1)
  {
  }
}


